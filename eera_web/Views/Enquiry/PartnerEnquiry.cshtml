@model List<eera_model.PartnerEnquiry>
@{
    ViewBag.Title = "PartnerEnquiry";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="col-md-12">
    <div class="box-inn-sp">
        <div class="inn-title">
            <h4>Partner Enquiry</h4>
        </div>
        <div class="tab-inn">
            <div class="table-responsive table-desi">
                @{
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>PartnerCode</th>
                                <th>UserCode</th>
                                <th>OrganizationName</th>
                                <th>Description</th>
                                <th>ContactNumber</th>
                                <th>MailId</th>
                                <th>Reply</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var partnerenquiry in Model)
                            {
                            <tr>
                                <td>@partnerenquiry.PartnerQueryCode</td>
                                <td>@partnerenquiry.UserCode</td>
                                <td>@partnerenquiry.OrganizationName</td>
                                <td>@partnerenquiry.Description</td>
                                <td>@partnerenquiry.PhoneNumber</td>
                                <td>@partnerenquiry.Email</td>
                                @*<td>
            @if (partnerenquiry.Status)
            {
                <span class="label label-success">Active</span>
            }
            else
            {
                <span class="label label-danger">Inactive</span>
            }
        </td>*@
                                <td> <a href="index-2.html"><i class="fa fa-reply" aria-hidden="true"></i> Reply</a></td>
                            </tr>
                            }
                        </tbody>
                    </table>
                }

            </div>
        </div>
    </div>
</div>
